{
  "name": "tweeter",
  "version": "1.0.0",
  "description": "tweeter is a twitter clone",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/server.js",
    "server": "nodemon ./src/server.js --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \" npm run server \" \" npm run client\"",
    "build-client": "cd client && npm run build"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.32.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tolufolorunso/Tweeter.git"
  },
  "author": "folorunso tolulope",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tolufolorunso/Tweeter/issues"
  },
  "homepage": "https://github.com/Tolufolorunso/Tweeter#readme",
  "engines": {
    "node": "16.x"
  }
}
